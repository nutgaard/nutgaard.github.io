{
  "name": "nutgaard.github.io",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:nutgaard/nutgaard.github.io.git",
  "author": "Nicklas Utgaard",
  "license": "MIT",
  "scripts": {
    "start": "DEV=true parcel src/index.html --out-dir build",
    "test": "ts-node src/statistics.ts",
    "build:inline": "node inline-resources.js build/index.html build/index.html",
    "build:app": "parcel build src/index.html --out-dir build",
    "build:static": "copyfiles -u 1 static/* build",
    "build": "npm run build:app && npm run build:static && npm run build:inline",
    "publish": "npm run build && gh-pages -d build -b master"
  },
  "devDependencies": {
    "cheerio": "^1.0.0-rc.2",
    "copyfiles": "^2.0.0",
    "gh-pages": "^1.1.0",
    "parcel-bundler": "^1.12.4",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4",
    "yet-another-fetch-mock": "^4.1.0"
  },
  "dependencies": {
    "src": "^1.1.2"
  }
}
